{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": "true" }]
  }
}
